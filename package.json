{
  "name": "@joethefkingfrypan/bk-conventional-changelog",
  "version": "2.2.1",
  "description": "Fork of conventional-changelog SportHeroesGroup backend preset",
  "main": "index.js",
  "scripts": {
    "lint": "jshint *.js --exclude node_modules && jscs *.js",
    "lint-win": "jshint index.js test.js --exclude node_modules && jscs index.js test.js",
    "test": "mocha --timeout 30000 && npm run-script lint",
    "test-only": "mocha --timeout 30000",
    "test-win": "mocha --timeout 30000 && npm run-script lint-win"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sportheroes/bk-conventional-changelog.git"
  },
  "keywords": [
    "conventional-changelog",
    "SportHeroesGroup",
    "preset"
  ],
  "author": "Romain Gagnaire <romain.gagnaire.pro@gmail.com>",
  "license": "ISC",
  "files": [
    "index.js",
    "usernames.js",
    "templates"
  ],
  "bugs": {
    "url": "https://github.com/sportheroes/bk-conventional-changelog/issues"
  },
  "homepage": "https://github.com/sportheroes/bk-conventional-changelog#readme",
  "peerDependencies": {
    "config": ">=3.X"
  },
  "devDependencies": {
    "chai": ">=4.5.0 <5.X",
    "config": "4.3.0",
    "conventional-changelog-core": ">=5.0.2 <6.X",
    "git-dummy-commit": "^1.3.0",
    "jscs": "3.0.7",
    "jshint": "2.13.6",
    "mocha": "11.7.5",
    "shelljs": "0.10.0",
    "through2": "4.0.2"
  },
  "dependencies": {
    "q": "1.5.1"
  }
}
